{
    "title": "Google Search Scraper Input",
    "type": "object",
    "schemaVersion": 1,
    "properties": {
        "searchTerms": {
            "title": "Search Terms",
            "type": "array",
            "description": "List of search terms to scrape",
            "editor": "stringList",
            "items": {
                "type": "string"
            },
            "minItems": 1,
            "default": [
                "gạch ốp lát giá rẻ"
            ]
        },
        "maxResults": {
            "title": "Max Results per Search Term",
            "type": "integer",
            "description": "Maximum number of results to scrape for each search term",
            "minimum": 1,
            "maximum": 100,
            "default": 10
        },
        "includeSnippets": {
            "title": "Include Snippets",
            "type": "boolean",
            "description": "Whether to include snippets/descriptions in the results",
            "default": true
        },
        "includeAds": {
            "title": "Include Ads",
            "type": "boolean",
            "description": "Whether to include paid advertisements in the results",
            "default": false
        },
        "searchEngine": {
            "title": "Search Engine",
            "type": "string",
            "description": "Search engine to use for scraping",
            "enum": [
                "google",
                "bing"
            ],
            "enumTitles": [
                "Google",
                "Bing"
            ],
            "default": "google"
        },
        "country": {
            "title": "Country Code",
            "type": "string",
            "description": "Country code for localized search results",
            "pattern": "^[a-z]{2}$",
            "editor": "textfield",
            "default": "vn"
        },
        "language": {
            "title": "Language Code",
            "type": "string",
            "description": "Language code for search interface",
            "pattern": "^[a-z]{2}$",
            "editor": "textfield",
            "default": "vi"
        },
        "proxyConfiguration": {
            "title": "Proxy configuration",
            "description": "Use Apify proxy or your own custom proxy settings",
            "type": "object",
            "editor": "proxy"
        },
        "maxConcurrency": {
            "title": "Max Concurrency",
            "type": "integer",
            "description": "Maximum number of concurrent requests",
            "minimum": 1,
            "maximum": 10,
            "default": 1
        },
        "requestDelay": {
            "title": "Request Delay (seconds)",
            "type": "integer",
            "description": "Delay between requests in seconds",
            "minimum": 1,
            "maximum": 60,
            "default": 3
        }
    },
    "required": [
        "searchTerms",
        "searchEngine",
        "country",
        "language"
    ]
}